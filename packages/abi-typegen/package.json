{
  "name": "@fuel-ts/abi-typegen",
  "version": "0.81.0",
  "description": "Generates Typescript definitions from Sway ABI Json files",
  "author": "Fuel Labs <contact@fuel.sh> (https://fuel.network/)",
  "bin": {
    "fuels-typegen": "typegen.js"
  },
  "engines": {
    "node": "^18.18.2 || ^20.0.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    },
    "./cli": {
      "import": {
        "types": "./dist/esm/cli.d.ts",
        "default": "./dist/esm/cli.js"
      },
      "require": {
        "types": "./dist/commonjs/cli.d.ts",
        "default": "./dist/commonjs/cli.js"
      }
    },
    "./runTypegen": {
      "import": {
        "types": "./dist/esm/runTypegen.d.ts",
        "default": "./dist/esm/runTypegen.js"
      },
      "require": {
        "types": "./dist/commonjs/runTypegen.d.ts",
        "default": "./dist/commonjs/runTypegen.js"
      }
    }
  },
  "typesVersions": {
    "*": {
      "cli": [
        "./dist/commonjs/cli.d.ts"
      ],
      "runTypegen": [
        "./dist/commonjs/runTypegen.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "pretest": "pnpm build:forc",
    "build": "tsup",
    "build:forc": "pnpm fuels-forc build -p test/fixtures/forc-projects --release",
    "postbuild": "tsx ../../scripts/postbuild.ts"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@fuel-ts/errors": "workspace:*",
    "@fuel-ts/interfaces": "workspace:^",
    "@fuel-ts/utils": "workspace:*",
    "@fuel-ts/versions": "workspace:*",
    "commander": "^9.4.1",
    "glob": "^10.2.6",
    "handlebars": "^4.7.7",
    "mkdirp": "^1.0.4",
    "ramda": "^0.29.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@fuel-ts/forc": "workspace:*",
    "@types/mkdirp": "^1.0.2",
    "@types/ramda": "^0.29.3",
    "@types/rimraf": "^3.0.2"
  },
  "tshy": {
    "exclude": [
      "**/*.test.ts"
    ],
    "include": [
      "**/*.hbs"
    ],
    "dialects": [
      "esm",
      "commonjs"
    ],
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts",
      "./cli": "./src/cli.ts",
      "./runTypegen": "./src/runTypegen.ts"
    }
  },
  "main": "./dist/commonjs/index.js",
  "types": "./dist/commonjs/index.d.ts",
  "type": "module"
}
